@model Web.Models.ViewedPost
@{
    ViewBag.Title = "SeenList";
}

<h2>SeenList</h2>

@foreach (var item in Model.Students)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>

    </tr>
}